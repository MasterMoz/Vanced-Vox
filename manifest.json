{
  
  "manifest_version": 3,
  "permissions": [],
  "icons":{
    "16":"img/icon-16.png",
    "48":"img/icon-48.png",
    "128":"img/icon-128.png" 
  },
  "name": "Vanced Voxiom",
  "short_name" : "V-Vox",
  "version": "0.1",
  "description" : "Make your experience at Voxiom.io all the more amazing!",
  "author" : "Namekuji",
  "content_scripts": [{
    "js": ["script.js"],
    "matches": ["*://voxiom.io/*"]
